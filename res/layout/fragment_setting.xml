<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    >
    <LinearLayout
        android:id="@+id/ll_setting_share" 
        android:layout_width="fill_parent"
        android:layout_height="80dip"
        android:layout_margin="10dip"
        android:background="#FAFAFA"
        android:gravity="center"
        android:orientation="vertical"
        >
        <ImageView 
            android:id="@+id/iv_setting_share" 
            android:layout_width="116dip"
            android:layout_height="36dip"
            android:layout_gravity="center"
            
            />         
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_setting_feedback" 
        android:layout_width="fill_parent"
        android:layout_height="80dip"
        android:layout_margin="10dip"
        android:background="#FAFAFA"
        android:gravity="center"
        android:orientation="vertical"
        >
        <ImageView 
            android:id="@+id/iv_setting_feedback" 
            android:layout_width="116dip"
            android:layout_height="36dip"
            android:layout_gravity="center"
            
            />           
    </LinearLayout>    
    <LinearLayout
        android:id="@+id/ll_setting_update" 
        android:layout_width="fill_parent"
        android:layout_height="80dip"
        android:layout_margin="10dip"
        android:background="#FAFAFA"
        android:gravity="center"
        android:orientation="vertical"
        >
        <ImageView 
            android:id="@+id/iv_setting_update"
            android:layout_width="116dip"
            android:layout_height="36dip"
            android:layout_gravity="center"
            
            />           
    </LinearLayout>  
    <LinearLayout
        android:id="@+id/ll_setting_about" 
        android:layout_width="fill_parent"
        android:layout_height="80dip"
        android:layout_margin="10dip"
        android:background="#FAFAFA"
        android:gravity="center"
        android:orientation="vertical"
        >       
        <ImageView 
            android:id="@+id/iv_setting_about"
            android:layout_width="116dip"
            android:layout_height="36dip"
            android:layout_gravity="center"
            
            />            
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_setting_like" 
        android:layout_width="fill_parent"
        android:layout_height="80dip"
        android:layout_margin="10dip"
        android:background="#FAFAFA"
        android:gravity="center"
        android:orientation="vertical"
        >       
        <ImageView 
            android:id="@+id/iv_setting_like"
            android:layout_width="116dip"
            android:layout_height="36dip"
            android:layout_gravity="center"
            
            />         
    </LinearLayout>


    

</LinearLayout>